<?xml version="1.0" encoding="UTF-8"?>
<interface>
<requires lib="libadwaita" version="1.6"/>

<object class="AdwWindow" id="window">
	<property name="default-width">1680</property>
	<property name="default-height">1050</property>
	<property name="width-request">1680</property>
	<property name="height-request">1050</property>
	<property name="content">
		<object class="AdwToolbarView" id="main_view">
			<child type="top">
				<object class="AdwHeaderBar">
					<child type="start">
						<object class="GtkBox">
							<property name="orientation">horizontal</property>
							<property name="halign">GTK_ALIGN_START</property>
							


							<child>
								<object class="GtkButton" id="tab_home">
									<property name="halign">GTK_ALIGN_CENTER</property>
									<property name="valign">GTK_ALIGN_CENTER</property>
									<property name="margin-end">10</property>
									<style>
										<class name="circular"/>
										<class name="raised"/>
									</style>
										<child>
											<object class="GtkImage">
												<property name="resource">/icon/home</property>
												<property name="pixel-size">20</property>
												<style>
													<class name="_circular"/>
												</style>
											</object>
										</child>
								</object>
							</child>


							<child>
								<object class="GtkButton" id="tab_like">
									<property name="halign">GTK_ALIGN_CENTER</property>
									<property name="valign">GTK_ALIGN_CENTER</property>
									<property name="margin-end">10</property>
									<style>
										<class name="circular"/>
										<class name="raised"/>
									</style>
										<child>
											<object class="GtkImage">
												<property name="resource">/icon/empty-like</property>
												<property name="pixel-size">20</property>
												<style>
													<class name="_circular"/>
												</style>
											</object>
										</child>
								</object>
							</child>


							<child>
								<object class="GtkButton" id="tab_dir">
									<property name="halign">GTK_ALIGN_CENTER</property>
									<property name="valign">GTK_ALIGN_CENTER</property>
									<property name="margin-end">10</property>
									<style>
										<class name="circular"/>
										<class name="raised"/>
									</style>
										<child>
											<object class="GtkImage">
												<property name="resource">/icon/playlist</property>
												<property name="pixel-size">20</property>
												<style>
													<class name="_circular"/>
												</style>
											</object>
										</child>
								</object>
							</child>


							<child>
								<object class="GtkButton">
									<property name="halign">GTK_ALIGN_CENTER</property>
									<property name="valign">GTK_ALIGN_CENTER</property>
									<property name="margin-end">10</property>
									<style>
										<class name="circular"/>
										<class name="raised"/>
									</style>
										<child>
											<object class="GtkImage">
												<property name="resource">/icon/search</property>
												<property name="pixel-size">20</property>
												<style>
													<class name="_circular"/>
												</style>
											</object>
										</child>
								</object>
							</child>
						

						</object>
					</child>
				</object>
			</child>
			
			<child type="bottom">
				<object class="GtkBox">
					<property name="orientation">horizontal</property>
					<property name="spacing">20</property>
					<property name="valign">GTK_ALIGN_CENTER</property>
					<style>
						<class name="_toolbar"/>
					</style>
					

					<child>
						<object class="GtkBox">
							<property name="orientation">vertical</property>
							<property name="spacing">25</property>
							<property name="valign">GTK_ALIGN_CENTER</property>

							<child>
								<object class="GtkLabel" id="title">
									<property name="label">Unknown Title</property>
									<property name="halign">GTK_ALIGN_START</property>
									<property name="xalign">0</property>
									<property name="max-width-chars">40</property>
									<property name="width-chars">40</property>
									<property name="lines">1</property>
									<property name="wrap">TRUE</property>
									<style>
										<class name="title-2"/>
									</style>
								</object>
							</child>

							<child>
								<object class="GtkLabel" id="artist">
									<property name="label">Unknown Artist</property>
									<property name="halign">GTK_ALIGN_START</property>
									<style>
										<class name="title-4"/>
									</style>
								</object>
							</child>
						</object>
					</child>
				
					<child>
						<object class="GtkButton" id="previous">
							<property name="halign">GTK_ALIGN_CENTER</property>
							<property name="valign">GTK_ALIGN_CENTER</property>
							<property name="margin-start">50</property>
							<style>
								<class name="circular"/>
								<class name="raised"/>
							</style>
							<child>
								<object class="GtkImage">
									<property name="resource">/icon/previous</property>
									<property name="pixel-size">20</property>
									<style>
										<class name="_circular"/>
									</style>
								</object>
							</child>
						</object>
					</child>

					<child>
						<object class="GtkButton" id="playpause">
							<property name="halign">GTK_ALIGN_CENTER</property>
							<property name="valign">GTK_ALIGN_CENTER</property>
							<style>
								<class name="circular"/>
								<class name="raises"/>
								<class name="suggested-action"/>
							</style>
							<child>
								<object class="GtkImage" id="playpause_image">
									<property name="resource">/icon/play</property>
									<property name="pixel-size">35</property>
									<style>
										<class name="_circular"/>
									</style>
								</object>
							</child>
						</object>
					</child>

					<child>
						<object class="GtkButton" id="next">
							<property name="halign">GTK_ALIGN_CENTER</property>
							<property name="valign">GTK_ALIGN_CENTER</property>
							<style>
								<class name="circular"/>
								<class name="raised"/>
							</style>
							<child>
								<object class="GtkImage">
									<property name="resource">/icon/next</property>
									<property name="pixel-size">20</property>
									<style>
										<class name="_circular"/>
									</style>
								</object>
							</child>
						</object>
					</child>

					<child>
						<object class="GtkLabel" id="position">
							<property name="label">00:00</property>
							<property name="margin-start">30</property>
							<style>
								<class name="title-4"/>
							</style>
						</object>
					</child>

					<child>
						<object class="GtkScale" id="progress_bar">
							<property name="valign">GTK_ALIGN_CENTER</property>
							<property name="hexpand">TRUE</property>
							<property name="show-fill-level">TRUE</property>
							<property name="restrict-to-fill-level">FALSE</property>
						</object>
					</child>


					<child>
						<object class="GtkLabel" id="duration">
							<property name="label">00:00</property>
							<style>
								<class name="title-4"/>
							</style>
						</object>
					</child>


					<child>
						<object class="GtkButton" id="volume">
							<property name="halign">GTK_ALIGN_CENTER</property>
							<property name="valign">GTK_ALIGN_CENTER</property>
							<style>
								<class name="circular"/>
								<class name="raised"/>
							</style>
							<child>
								<object class="GtkImage">
									<property name="resource">/icon/sound</property>
									<property name="pixel-size">20</property>
									<style>
										<class name="_circular"/>
									</style>
								</object>
							</child>
						</object>
					</child>


					<child>
						<object class="GtkButton" id="repeat">
							<property name="halign">GTK_ALIGN_CENTER</property>
							<property name="valign">GTK_ALIGN_CENTER</property>
							<style>
								<class name="circular"/>
								<class name="raised"/>
							</style>
							<child>
								<object class="GtkImage">
									<property name="resource">/icon/repeat</property>
									<property name="pixel-size">20</property>
									<style>
										<class name="_circular"/>
									</style>
								</object>
							</child>
						</object>
					</child>
					
					<child>
						<object class="GtkButton" id="like">
							<property name="halign">GTK_ALIGN_CENTER</property>
							<property name="valign">GTK_ALIGN_CENTER</property>
							<property name="margin-end">10</property>
							<style>
								<class name="circular"/>
								<class name="raised"/>
							</style>
							<child>
								<object class="GtkImage">
									<property name="resource">/icon/empty-like</property>
									<property name="pixel-size">20</property>
									<style>
										<class name="_circular"/>
									</style>
								</object>
							</child>
						</object>
					</child>
				</object>
			</child>

		</object>	
	</property>
</object>

</interface>
