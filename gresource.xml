<?xml version="1.0" encoding="UTF-8"?>

<gresources>
	<gresource prefix="/ui">
		<file preprocess="xml-stripblanks" alias="splash">ui/splash.ui</file>
		<file preprocess="xml-stripblanks" alias="main">ui/main.ui</file>
		<file preprocess="xml-stripblanks" alias="dir_empty">ui/dir_empty.ui</file>
		<file preprocess="xml-stripblanks" alias="music_add_load">ui/music_add_load.ui</file>
		<file preprocess="xml-stripblanks" alias="home">ui/home.ui</file>
		<file preprocess="xml-stripblanks" alias="music_item">ui/music_item.ui</file>
		<file preprocess="xml-stripblanks" alias="volume">ui/volume.ui</file>
		<file preprocess="xml-stripblanks" alias="dir_dialog">ui/dir_dialog.ui</file>
	</gresource>

	<gresource prefix="/css">
		<file alias="global-style">ui/css/style.css</file>
	</gresource>

	<gresource prefix="/image">
		<file alias="splash-logo">ui/image/splash_logo.svg</file>
		<file alias="empty-music">ui/image/empty_music.svg</file>
	</gresource>

	<gresource prefix="/icon">
		<file alias="empty-like">ui/icon/empty_like.svg</file>
		<file alias="previous">ui/icon/prev.svg</file>
		<file alias="play">ui/icon/play.svg</file>
		<file alias="next">ui/icon/next.svg</file>
		<file alias="sound">ui/icon/sound.svg</file>
		<file alias="repeat">ui/icon/repeat.svg</file>
		<file alias="playlist">ui/icon/playlist.svg</file>
		<file alias="search">ui/icon/search.svg</file>
		<file alias="home">ui/icon/home.svg</file>
		<file alias="pause">ui/icon/pause.svg</file>
	</gresource>

</gresources>
